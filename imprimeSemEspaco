class Program
    {
		static void imprimeSemEspaco(string str)
		{
			for (int i = 0; i < str.Length; i++)
			{
				if (str[i] != ' ')
					Console.Write(str[i]);
			}
		}
		static void Main(string[] args)
        {
			string str = "AnÃ¡lise e Desenvolvimento de Sistemas";
			imprimeSemEspaco(str);
		}
    }
